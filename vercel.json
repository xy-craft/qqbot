{
    "version": 2,
    "builds": [
        {
            "src": "bot.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bot.py",
            "methods": ["GET", "POST"]
        }
    ]
}