{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "supportsResponseStreaming": true
      }
    }
  ],
  "routes": [
    {
      "src": "/qq/receive",
      "dest": "bot.py",
      "methods": ["GET", "POST", "WEBSOCKET"]
    }
  ]
}