{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "includeFiles": ["*.env"]
      }
    }
  ],
  "routes": [
    {
      "src": "/qq/receive",
      "dest": "bot.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "bot.py",
      "methods": ["GET"]
    }
  ]
}